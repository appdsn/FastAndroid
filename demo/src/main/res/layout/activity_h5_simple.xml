<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white">

    <Button
        android:id="@+id/h5_fragment_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="doClick"
        android:text="h5嵌入在Fragment使用" />

    <Button
        android:id="@+id/h5_activity_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="doClick"
        android:text="H5嵌入在Activity使用" />

    <Button
        android:id="@+id/h5_in_tab_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="doClick"
        android:text="H5嵌入在Tab内Fragment使用" />

    <Button
        android:id="@+id/h5_base_activity_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="doClick"
        android:text="BaseWebActivity使用" />

</LinearLayout>